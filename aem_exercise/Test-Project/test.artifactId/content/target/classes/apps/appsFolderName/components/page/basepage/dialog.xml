<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Dialog"
    height="400"
    stateful="false"
    xtype="dialog">
    <items jcr:primaryType="cq:WidgetCollection">
        <tabs jcr:primaryType="cq:TabPanel">
            <items jcr:primaryType="cq:WidgetCollection">
                <basic
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/foundation/components/page/tab_basic.infinity.json"
                    xtype="cqinclude"/>
                <advanced
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/foundation/components/page/tab_advanced.infinity.json"
                    xtype="cqinclude"/>
                <image
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/foundation/components/page/tab_image.infinity.json"
                    xtype="cqinclude"/>
                <cloudservices
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/foundation/components/page/tab_cloudservices.infinity.json"
                    xtype="cqinclude"/>
                <blueprint
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/foundation/components/page/tab_blueprint.infinity.json"
                    xtype="cqinclude"/>
                <livecopy
                    jcr:primaryType="cq:Widget"
                    path="/libs/wcm/foundation/components/page/tab_livecopy.infinity.json"
                    xtype="cqinclude"/>
                <alcs
                    jcr:primaryType="cq:Panel"
                    title="Alcs">
                    <items jcr:primaryType="cq:WidgetCollection">
                        <advanced
                            jcr:primaryType="cq:Widget"
                            collapsed="{Boolean}false"
                            collapsible="{Boolean}false"
                            xtype="dialogfieldset">
                            <items jcr:primaryType="cq:WidgetCollection">
                                <contentType
                                    jcr:primaryType="cq:Widget"
                                    fieldLabel="Content Type"
                                    name="./contentType"
                                    type="select"
                                    xtype="selection">
                                    <options jcr:primaryType="cq:WidgetCollection">
                                        <content
                                            jcr:primaryType="nt:unstructured"
                                            text="Content"
                                            value="content"/>
                                        <home
                                            jcr:primaryType="nt:unstructured"
                                            text="Home"
                                            value="home"/>
                                        <blogListing
                                            jcr:primaryType="nt:unstructured"
                                            text="Blog Listing"
                                            value="blogListing"/>
                                        <blogDetail
                                            jcr:primaryType="nt:unstructured"
                                            text="Blog Detail"
                                            value="blogDetail"/>
                                        <profile
                                            jcr:primaryType="nt:unstructured"
                                            text="Profile"
                                            value="profile"/>
                                        <offers
                                            jcr:primaryType="nt:unstructured"
                                            text="Offers"
                                            value="offers"/>
                                    </options>
                                </contentType>
                            </items>
                        </advanced>
                    </items>
                </alcs>
            </items>
        </tabs>
    </items>
</jcr:root>
